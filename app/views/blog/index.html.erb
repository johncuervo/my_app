<h1 align="center"> Blogs </h1>

<%= link_to "Create new Post", new_blog_path%>

<br>

<table align="center">
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
    </tr>
  </thead>
  <body>
    <% @blogs.each do |blog| %>
      <tr>
        <td><%= blog.title %></td>
        <td><%= blog.content %></td>
        <td><%= link_to "Show", blog_path(blog.id)%></td>
        <td><%= link_to "Edit", edit_blog_path(blog.id)%></td>
        <td><%= link_to "Delete", blog_path(blog.id), method: :delete, data: {confirm:"Esta seguro de eliminar"}%></td>
      </tr>
    <%end%>
  </body>
</table>






